class container <T extends Number>{
T value;
    public T getvalue(){
    return value;
}
public void setValue(T value){
        this.value=value;
}
public void display(){
        System.out.println(value);
}

}

public class generics {
    public static void main(String[] args) {
container<Integer> obj = new container<>();
obj.setValue(2);
obj.display();
    }
}
